{
  "name": "webpack4-react",
  "version": "1.0.0",
  "description": "webpack4-react",
  "private": true,
  "scripts": {
    "dev": "npm run dev:server",
    "dev2": "npm run dev:server & npm run mock & npm run start:dev",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --open --config webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.production.js",
    "dll": "cross-env NODE_ENV=production webpack --config webpack/webpack.dll.config.js",
    "mock": "cd mock && node ./proxy/index.js",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "clean": "rm -r dist/*"
  },
  "keywords": [
    "webpack",
    "webapck4",
    "React"
  ],
  "author": "gengliangxiang",
  "license": "ISC",
